<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=1280, height=720, initial-scale=1.0">
  <title>draw</title>
  <link rel="stylesheet" href="../include/css/style.css">
  <link rel="stylesheet" href="../include/css/apng.css">
  <script src="../include/js/pageManager.js"></script>
</head>
<body>
  <div class="wrap">
    <main>
      <section class="contents">
        <div class="js-apng apngContainer" data-src="../include/images/apng/sample.png" data-auto-play="true" data-loop="true"></div>
      </section>
    </main>
  </div>
  <script>
    document.addEventListener('DOMContentLoaded', () => {
      // setting apng callback
      const apng = new APNG(getEl('.js-apng')[0]);
      loop(() => apng.player).then((player) => {
        apng.callback = {
          timeupdate: (APNG) => {
            console.log(APNG.currentFrameNumber);
          }
        }
      });
    });
  </script>
</body>
</html>